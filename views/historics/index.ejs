<h3>Pfft...Dinos? Gimme historics!</h3>
<h4>Let's do this!</h4>
<ul>
    <% historics.forEach((historic, creatureIndex) => { %>
        <li>
            <p><%= historic.type %></p>
            <img width=400 src='<%=historic.img_url%>'>

            <form 
              method="POST" 
              action="/prehistoric_creatures/<%= creatureIndex %>?_method=DELETE">
              <input type="submit" value="Delete">
          </form>
        </li>
    <% }) %>
</ul>